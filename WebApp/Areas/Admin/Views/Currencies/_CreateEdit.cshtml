@model BLL.App.DTO.Currency

<div asp-validation-summary="All" class="text-danger"></div>
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Abbreviation" class="control-label"></label>
    <input asp-for="Abbreviation" class="form-control" />
    <span asp-validation-for="Abbreviation" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Symbol" class="control-label"></label>
    <input asp-for="Symbol" class="form-control" />
    <span asp-validation-for="Symbol" class="text-danger"></span>
</div>
